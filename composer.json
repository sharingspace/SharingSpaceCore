{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
      "aws/aws-sdk-php-laravel": "~3.0",
      "barryvdh/laravel-debugbar": "^2.0",
      "cartalyst/stripe-billing-laravel": "~4.0",
      "cviebrock/eloquent-sluggable": "^3.0",
      "erusev/parsedown": "dev-master",
      "graham-campbell/digitalocean": "~2.1",
      "laravelcollective/html" : "5.*",
      "intervention/image": "2.*",
      "jenssegers/rollbar": "^1.4",
      "laravel/framework": "5.2.*",
      "laravel/socialite": "^2.0",
      "php": ">=5.5.9",
      "yaap/theme": "2.*",
      "chrisbjr/api-guard": "3.1.*",
      "mcamara/laravel-localization": "1.0.*",
      "watson/validating": "^2.0",
        "doctrine/dbal": "^2.5"
    },
    "repositories": [
    { "type": "composer", "url": "https://packages.cartalyst.com" }
    ],
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1",
        "codeception/codeception": "2.1.*",
        "symfony/dom-crawler": "~3.0",
        "symfony/css-selector": "~3.0"
    },
    "autoload": {
        "classmap": [
            "database",
            "app/Models",
            "app/Http/Transformers",
            "app/Helpers/Helper.php",
            "app/Traits"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan optimize"
        ],
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
