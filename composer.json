{
  "name": "snipe/massmosaic-l5",
  "description": "AnyShare Codebase",
  "keywords": [
    "sharing economy",
    "social network"
  ],
  "license": "Private",
  "type": "project",
  "require": {
    "aws/aws-sdk-php-laravel": "~3.0",
    "barryvdh/laravel-debugbar": "^2.0",
    "cartalyst/stripe-billing-laravel": "6.0",
    "cviebrock/eloquent-sluggable": "^3.0",
    "graham-campbell/markdown": "^7.0",
    "laravelcollective/html": "5.*",
    "intervention/image": "2.*",
    "jenssegers/rollbar": "^1.4",
    "laravel/framework": "5.4.*",
    "laravel/socialite": "^3.0",
    "php": ">=5.5.9",
    "yaap/theme": "2.*",
    "chrisbjr/api-guard": "3.1.*",
    "mcamara/laravel-localization": "1.2.*",
    "watson/validating": "^2.0",
    "doctrine/dbal": "2.6.3",
    "laravel/tinker": "^1.0",
    "spatie/laravel-blade-javascript": "1.1.0",
    "guzzlehttp/guzzle": "~6.0",
    "dingo/api": "2.0.0-alpha1",
    "barryvdh/laravel-cors": "^0.11.0",
    "spatie/laravel-permission": "^2.12"

  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.cartalyst.com"
    }
  ],
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~5.7",
    "phpspec/phpspec": "~2.1",
    "codeception/codeception": "^2.3",
    "symfony/dom-crawler": "~3.1",
    "symfony/css-selector": "~3.1"
  },
  "autoload": {
    "classmap": [
      "database",
      "app/Http/Transformers",
      "app/Helpers/Helper.php",
      "app/Helpers/Permission.php",
      "app/Traits"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "php artisan optimize"
    ],
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
